platform :ios, '9.0'

inhibit_all_warnings!
use_frameworks!

target 'SwiftAppSample' do
    
    pod 'Alamofire', '~> 4.8.0'      # 网络库
    pod 'RxCocoa', '~> 4.4.0'        # cocoa扩展
    pod 'RxSwift', '~> 4.4.0'        # rx
    pod 'Moya', '~> 12.0.0'          # 网络库
    pod 'Moya/RxSwift'               # rxw化的网络库
    pod 'RxDataSources', '~> 3.1.0'  # 数据源
    pod 'Kingfisher', '~> 4.10.0'    # 图片库
    pod 'Then', '~> 2.4.0'           # 一些语法糖
    pod 'SnapKit', '~> 4.0.1'        # 布局库
    pod 'SwiftyJSON', '~> 4.2.0'     # json
    pod 'Dip', '~> 7.0.0'            # ioc 容器
    pod 'QMUIKit', '~> 2.9.2'
    pod 'Closures', '~> 0.5'
    pod 'MJRefresh', '~> 3.1.15.7'   # 刷新库
    pod 'EFQRCode', '~> 4.4.2'       # 二维码
end
